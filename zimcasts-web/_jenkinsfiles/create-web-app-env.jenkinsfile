pipeline {
    agent any
    environment {
        ENVIRONMENT_DIR = 'zimcasts'
        ENVIRONMENT_TYPE = 'dev'
        DOCKER_COMPOSE_FILE = 'compose-web-env.yml'
    }    
    stages {
        stage('Create Web App Resources') {
            steps {
                sh "docker compose -p ${env.ENVIRONMENT_DIR}-${env.ENVIRONMENT_TYPE} -f ${env.DOCKER_COMPOSE_FILE} up -d --build"
            }
        }

    }
}